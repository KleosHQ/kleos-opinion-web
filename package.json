{
  "name": "kleos-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "prisma generate",
    "dev": "NODE_OPTIONS='--disable-warning=ExperimentalWarning' next dev",
    "postinstall": "prisma generate",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate:client": "npx codama run --all",
    "sync:idl": "mkdir -p target/idl target/types && cp ../kleos-protocol/target/idl/kleos_protocol.json target/idl/ && cp ../kleos-protocol/target/types/kleos_protocol.ts target/types/",
    "sync:all": "pnpm run sync:idl && pnpm run generate:client",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:sync": "prisma generate && prisma db push"
  },
  "dependencies": {
    "@codama/nodes-from-anchor": "^1.3.9",
    "@codama/renderers-js": "^1.7.0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@privy-io/react-auth": "^3.13.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@solana-program/memo": "*",
    "@solana-program/system": "*",
    "@solana-program/token": "*",
    "@solana/kit": "*",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.0",
    "axios": "^1.13.5",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "js-sha3": "^0.8.0",
    "lucide-react": "^0.575.0",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@codama/cli": "^1.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.0",
    "postcss": "^8.4.47",
    "prisma": "^7.4.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
